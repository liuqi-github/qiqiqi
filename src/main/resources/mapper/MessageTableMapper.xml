<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.thread.demo.mapper.MessageTableMapper">
    <select id="findReSendMessageId" resultType="java.lang.Integer">
        select id
        from message_table
        where status = 1
    </select>
</mapper>